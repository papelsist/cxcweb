{
  "version": 1,
  "projects": {
    "cxc-web": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/cxc-web",
      "sourceRoot": "apps/cxc-web/src",
      "prefix": "nx-papelsa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/cxc-web",
            "index": "apps/cxc-web/src/index.html",
            "main": "apps/cxc-web/src/main.ts",
            "polyfills": "apps/cxc-web/src/polyfills.ts",
            "tsConfig": "apps/cxc-web/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/cxc-web/src/favicon.ico",
              "apps/cxc-web/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/cxc-web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cxc-web/src/environments/environment.ts",
                  "with": "apps/cxc-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cxc-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cxc-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cxc-web:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/cxc-web/tsconfig.app.json",
              "apps/cxc-web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/cxc-web/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/cxc-web/jest.config.js",
            "tsConfig": "apps/cxc-web/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/cxc-web/src/test-setup.ts"
          }
        }
      }
    },
    "cxc-web-e2e": {
      "root": "apps/cxc-web-e2e",
      "sourceRoot": "apps/cxc-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cxc-web-e2e/cypress.json",
            "tsConfig": "apps/cxc-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "cxc-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cxc-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/cxc-web-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/cxc-web-e2e/**/*"]
          }
        }
      }
    },
    "cxc-feature-cartera": {
      "projectType": "library",
      "root": "libs/cxc/feature-cartera",
      "sourceRoot": "libs/cxc/feature-cartera/src",
      "prefix": "papx-cxc",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/cxc/feature-cartera/tsconfig.lib.json",
              "libs/cxc/feature-cartera/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/cxc/feature-cartera/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/cxc/feature-cartera/jest.config.js",
            "tsConfig": "libs/cxc/feature-cartera/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/cxc/feature-cartera/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "shared-utils-collections": {
      "root": "libs/shared/utils/collections",
      "sourceRoot": "libs/shared/utils/collections/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/shared/utils/collections/tsconfig.lib.json",
              "libs/shared/utils/collections/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/shared/utils/collections/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/utils/collections/jest.config.js",
            "tsConfig": "libs/shared/utils/collections/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "cxc-web"
}
